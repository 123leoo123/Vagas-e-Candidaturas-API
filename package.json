{
  "name": "vagas-e-candidaturas-api",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start": "node dist/server.js",
    "biuld": "npx tsc",
    "dev": "dotenv -e .env.develop -- tsnd --respawn src/server.ts",
    "migrate:dev": "dotenv -e .env.develop -- npx prisma migrate dev",
    "test": "dotenv -e .env.test -- tsnd --respawn src/server.ts",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev",
    "studio": "dotenv -e .env.develop -- npx prisma studio",
    "studio:test": "dotenv -e .env.test -- npx prisma studio",
    "test:unit": "dotenv -e .env.test -- jest --config jest.config.unit.ts --verbose --runInBand",
    "test:integration": "dotenv -e .env.test -- jest --config jest.config.integration.ts --verbose --runInBand"
  },
  
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prisma": "^5.19.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "uuid": "^10.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "todo": "^1.1.0",
    "tsyringe": "^4.8.0",
    "zod": "^3.23.8"
  }
}
